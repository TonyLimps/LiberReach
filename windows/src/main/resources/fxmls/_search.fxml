<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="1.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <TextField fx:id="searchField" promptText="%main.textField.search" HBox.hgrow="ALWAYS" />
            <Button fx:id="searchButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="%main.button.search" HBox.hgrow="NEVER" />
         </children>
      </HBox>
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <ListView maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <ScrollBar orientation="VERTICAL" HBox.hgrow="NEVER" />
         </children>
      </HBox>
   </children>
</VBox>
