<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.tonylimps.liberreach.windows.controllers.AddController">
   <children>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="%add.label.address" />
            <TextField fx:id="addressField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="300.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="50.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_RIGHT" VBox.vgrow="ALWAYS">
         <children>
            <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="%add.label.token" HBox.hgrow="ALWAYS" />
            <TextArea fx:id="tokenArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="300.0" wrapText="true">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </TextArea>
         </children>
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="20.0" />
         </VBox.margin>
      </HBox>
      <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddButtonAction" text="%add.button.add">
         <VBox.margin>
            <Insets bottom="50.0" right="50.0" top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
